{
  "name": "express-typescript-starter",
  "version": "1.0.0",
  "description": "A starter for Express with TypeScript, including Prisma, Zod, jsonwebtoken and more.",
  "main": "src/server.ts",
  "scripts": {
    "build": "tsc --build",
    "start": "node ./dist/server.js",
    "dev": "node -r ts-node/register --watch --env-file=.env ./src/server.ts",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "keywords": [],
  "author": "Lorenzo Lucchesi",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.2.0",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^5.0.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "ts-patch": "^3.3.0",
    "typescript-transform-paths": "^3.5.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.5",
    "eslint": "^9.17.0",
    "prisma": "^6.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
